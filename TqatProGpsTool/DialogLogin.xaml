<Window x:Class="TqatProGpsTool.DialogLogin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TqatProGpsTool"
        mc:Ignorable="d"
        Title="DialogLogin" Height="200" Width="400"
        WindowStyle="ToolWindow"
        
        xmlns:UserControls="clr-namespace:Controls.UserControls"
        >
    <Grid>
        <UserControls:PanelLogin x:Name="panelLogin" 
                VerticalAlignment="Bottom" 
                OnSubmitEventHandler="PanelLogin_OnSubmitEventHandler" 
                OnCancelEventHandler="PanelLogin_OnCancelEventHandler">
        </UserControls:PanelLogin>
     
    </Grid>
</Window>
